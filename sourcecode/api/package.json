{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "start:dev": "ts-node-dev --respawn --transpile-only src/api.ts",
    "build": "rm -rf dist && tsc && yarn copy-non-ts-files",
    "copy-non-ts-files": "copyfiles -u 1 src/**/*.hbs dist/",
    "migrate": "yarn build && yarn migrate:prod",
    "migrate:prod": "node dist/runMigrations.js",
    "knex:migration:create": "knex migrate:make --migrations-directory src/migrations"
  },
  "dependencies": {
    "@google-cloud/logging-winston": "^4.1.0",
    "@google-cloud/storage": "^6.4.0",
    "@graphql-tools/merge": "^8.3.3",
    "@graphql-tools/schema": "^8.5.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcrypt": "^5.0.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "cookie-parser": "^1.4.6",
    "copyfiles": "^2.4.1",
    "dataloader": "^2.1.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "fs-extra": "^10.1.0",
    "graphql": "^16.5.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-tag": "^2.12.6",
    "handlebars": "^4.7.7",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "jszip": "^3.10.1",
    "knex": "^0.95.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.7.3",
    "prettier": "^2.7.1",
    "redis": "^3.1.2",
    "stripe": "^10.7.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  }
}
